{
 "extends": ["airbnb"],
 "rules": {
   "arrow-body-style": 0,
   "semi": 0,
   "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
   "react/prop-types": 0,
   "object-curly-newline": ["error", {
        "ObjectExpression": "always",
        "ObjectPattern": { "multiline": true }
    }]
   }
}
